{
  "name": "veterina-reservations",
  "private": true,
  "version": "1.0.0",
  "description": "Multi-tenant rezervační systém pro veterinární ordinace",
  "scripts": {
    "dev": "concurrently \"npm run dev:api\" \"npm run dev:web\"",
    "dev:web": "cd apps/web && npm run dev",
    "dev:api": "cd apps/api && npm run dev",
    "build": "npm run build:web && npm run build:api",
    "build:web": "cd apps/web && npm run build",
    "build:api": "cd apps/api && npm run build",
    "db:setup": "npx prisma generate && npx prisma db push && npx prisma db seed",
    "install:all": "cd apps/web && npm install && cd ../api && npm install"
  },
  "devDependencies": {
    "@types/node": "^24.0.7",
    "concurrently": "^8.2.0",
    "prisma": "^6.10.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  },
  "keywords": [
    "veterinary",
    "reservations",
    "multi-tenant",
    "nextjs",
    "prisma",
    "oauth"
  ],
  "author": "Veterina Reservations",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^6.10.1",
    "@types/bcryptjs": "^2.4.6",
    "autoprefixer": "^10.4.21",
    "bcryptjs": "^3.0.2",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.11"
  }
}
